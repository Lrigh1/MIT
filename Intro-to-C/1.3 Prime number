/*Write a program to read a number N from the user and then find the first N primes. A
prime number is a number that only has two divisors, one and itself.
------------------------------------------------------------------------
assumes a prime number is being entered for the most part*/


#include <iostream>

using namespace std;

int main()
{
	int N;

	cout << "Please enter a prime number\n";
	cin >> N;
	for(int i=1; i <= N; i++)
	{
		if(i % N == 0)
		{
		cout << i << endl;
		}
	}

system("pause");
}
